<p id="notice"><%= notice %></p>

<h1>Historical Boxes</h1>

<table>
  <thead>
    <tr>
      <th>Ticket box</th>
      <th>Status</th>
      <th>Available</th>
      <th>Not available</th>
      <th>Inflow detail</th>
      <th>Outflow integer</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @historical_boxes.each do |historical_box| %>
      <tr>
        <td><%= historical_box.ticket_box %></td>
        <td><%= historical_box.status %></td>
        <td><%= historical_box.available %></td>
        <td><%= historical_box.not_available %></td>
        <td><%= historical_box.inflow_detail_id %></td>
        <td><%= historical_box.outflow_integer_id %></td>
        <td><%= link_to 'Show', historical_box %></td>
        <td><%= link_to 'Edit', edit_historical_box_path(historical_box) %></td>
        <td><%= link_to 'Destroy', historical_box, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Historical Box', new_historical_box_path %>
