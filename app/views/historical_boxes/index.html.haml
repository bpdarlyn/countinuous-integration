%p#notice= notice
%h1 Historical Boxes
%table
  %thead
    %tr
      %th Ticket box
      %th Status
      %th Available
      %th Not available
      %th Inflow detail
      %th Outflow integer
      %th{:colspan => "3"}
  %tbody
    - @historical_boxes.each do |historical_box|
      %tr
        %td= historical_box.ticket_box
        %td= historical_box.status
        %td= historical_box.available
        %td= historical_box.not_available
        %td= historical_box.inflow_detail_id
        %td= historical_box.outflow_integer_id
        %td= link_to 'Show', historical_box
        %td= link_to 'Edit', edit_historical_box_path(historical_box)
        %td= link_to 'Destroy', historical_box, method: :delete, data: { confirm: 'Are you sure?' }
%br/
= link_to 'New Historical Box', new_historical_box_path
