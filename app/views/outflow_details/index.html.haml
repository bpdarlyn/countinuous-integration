%p#notice= notice
%h1 Outflow Details
%table
  %thead
    %tr
      %th Outflow
      %th Product
      %th Quantity
      %th Start number
      %th End number
      %th{:colspan => "3"}
  %tbody
    - @outflow_details.each do |outflow_detail|
      %tr
        %td= outflow_detail.outflow
        %td= outflow_detail.product
        %td= outflow_detail.quantity
        %td= outflow_detail.start_number
        %td= outflow_detail.end_number
        %td= link_to 'Show', outflow_detail
        %td= link_to 'Edit', edit_outflow_detail_path(outflow_detail)
        %td= link_to 'Destroy', outflow_detail, method: :delete, data: { confirm: 'Are you sure?' }
%br/
= link_to 'New Outflow Detail', new_outflow_detail_path
