<tr class="nested-fields">
	<td>
		<%= f.select :product_id, Product.all.map{ |p| [p.name, p.id]}, { prompt: "Seleccione un Producto"}, { class: "form-control"} %>
	</td>
	<td>
		<%= f.text_field :ticket_boxes_id, class: 'form-control' %>
	</td>
	<td>
		<%= f.text_field :start_number, class: 'form-control' %>
	</td>
	<td>
		<%= f.text_field :end_number, class: 'form-control' %>
	</td>
	<td>
		<%= f.text_field :quantity, class: 'form-control' %>
	</td>
	<td>
		<%= link_to_remove_association "X", f, class: "btn btn-danger btn-xs" %>
	</td>
</tr>