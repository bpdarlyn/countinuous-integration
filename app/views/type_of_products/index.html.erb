<p id="notice"><%= notice %></p>

<h1>Type Of Products</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @type_of_products.each do |type_of_product| %>
      <tr>
        <td><%= type_of_product.name %></td>
        <td><%= link_to 'Show', type_of_product %></td>
        <td><%= link_to 'Edit', edit_type_of_product_path(type_of_product) %></td>
        <td><%= link_to 'Destroy', type_of_product, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Type Of Product', new_type_of_product_path %>
