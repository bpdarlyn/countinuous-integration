<p id="notice"><%= notice %></p>

<h1>Type Of Events</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @type_of_events.each do |type_of_event| %>
      <tr>
        <td><%= type_of_event.name %></td>
        <td><%= link_to 'Show', type_of_event %></td>
        <td><%= link_to 'Edit', edit_type_of_event_path(type_of_event) %></td>
        <td><%= link_to 'Destroy', type_of_event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Type Of Event', new_type_of_event_path %>
