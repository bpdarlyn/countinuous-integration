%p#notice= notice
%h1 Type Of Events
%table
  %thead
    %tr
      %th Name
      %th{:colspan => "3"}
  %tbody
    - @type_of_events.each do |type_of_event|
      %tr
        %td= type_of_event.name
        %td= link_to 'Show', type_of_event
        %td= link_to 'Edit', edit_type_of_event_path(type_of_event)
        %td= link_to 'Destroy', type_of_event, method: :delete, data: { confirm: 'Are you sure?' }
%br/
= link_to 'New Type Of Event', new_type_of_event_path
