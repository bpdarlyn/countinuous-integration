%p#notice= notice
%h1 Outflows
%table
  %thead
    %tr
      %th Outflow date
      %th Orden request
      %th{:colspan => "3"}
  %tbody
    - @outflows.each do |outflow|
      %tr
        %td= outflow.outflow_date
        %td= outflow.orden_request_id
        %td= link_to 'Show', outflow
        %td= link_to 'Edit', edit_outflow_path(outflow)
        %td= link_to 'Destroy', outflow, method: :delete, data: { confirm: 'Are you sure?' }
%br/
= link_to 'New Outflow', new_outflow_path
